{
  "name": "ngx-masonry",
  "version": "14.0.1",
  "description": "Angular Module for displaying a feed of items in a masonry layout using https://github.com/desandro/masonry",
  "keywords": [
    "angular",
    "angular 2",
    "masonry",
    "masonry layout",
    "masonry feed",
    "desandro",
    "masonry",
    "ngx"
  ],
  "homepage": "https://github.com/wynfred/ngx-masonry/",
  "bugs": {
    "url": "https://github.com/wynfred/ngx-masonry/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wynfred/ngx-masonry/"
  },
  "peerDependencies": {
    "@angular/common": ">8.0.0",
    "@angular/core": ">8.0.0",
    "masonry-layout": "^4.2.0"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "module": "fesm2015/ngx-masonry.mjs",
  "es2020": "fesm2020/ngx-masonry.mjs",
  "esm2020": "esm2020/ngx-masonry.mjs",
  "fesm2020": "fesm2020/ngx-masonry.mjs",
  "fesm2015": "fesm2015/ngx-masonry.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-masonry.mjs",
      "es2020": "./fesm2020/ngx-masonry.mjs",
      "es2015": "./fesm2015/ngx-masonry.mjs",
      "node": "./fesm2015/ngx-masonry.mjs",
      "default": "./fesm2020/ngx-masonry.mjs"
    }
  },
  "sideEffects": false
}