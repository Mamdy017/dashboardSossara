{
  "name": "@ctrl/ngx-emoji-mart",
  "version": "7.1.0",
  "homepage": "https://ngx-emoji-mart.vercel.app",
  "repository": "scttcper/ngx-emoji-mart",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "description": "Customizable Slack-like emoji picker for Angular",
  "peerDependencies": {
    "@angular/core": ">=14.0.0-0"
  },
  "exports": {
    "./picker": {
      "default": "./picker.css"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ctrl-ngx-emoji-mart.mjs",
      "es2020": "./fesm2020/ctrl-ngx-emoji-mart.mjs",
      "es2015": "./fesm2015/ctrl-ngx-emoji-mart.mjs",
      "node": "./fesm2015/ctrl-ngx-emoji-mart.mjs",
      "default": "./fesm2020/ctrl-ngx-emoji-mart.mjs"
    },
    "./ngx-emoji": {
      "types": "./ngx-emoji/index.d.ts",
      "esm2020": "./esm2020/ngx-emoji/ctrl-ngx-emoji-mart-ngx-emoji.mjs",
      "es2020": "./fesm2020/ctrl-ngx-emoji-mart-ngx-emoji.mjs",
      "es2015": "./fesm2015/ctrl-ngx-emoji-mart-ngx-emoji.mjs",
      "node": "./fesm2015/ctrl-ngx-emoji-mart-ngx-emoji.mjs",
      "default": "./fesm2020/ctrl-ngx-emoji-mart-ngx-emoji.mjs"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "angular",
    "emoji",
    "picker"
  ],
  "module": "fesm2015/ctrl-ngx-emoji-mart.mjs",
  "es2020": "fesm2020/ctrl-ngx-emoji-mart.mjs",
  "esm2020": "esm2020/ctrl-ngx-emoji-mart.mjs",
  "fesm2020": "fesm2020/ctrl-ngx-emoji-mart.mjs",
  "fesm2015": "fesm2015/ctrl-ngx-emoji-mart.mjs",
  "typings": "index.d.ts",
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
