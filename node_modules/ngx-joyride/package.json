{
  "name": "ngx-joyride",
  "version": "2.5.0",
  "description": "An Angular Tour (Joyride) library built entirely in Angular, without using any heavy external dependencies like Bootstrap or JQuery. From now on you can easily guide your users through your site showing them all the sections and features.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnicola/ngx-joyride.git"
  },
  "keywords": [
    "tour",
    "joyride",
    "angular",
    "demo",
    "onboarding",
    "directive"
  ],
  "author": "Nicola Tommasi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tnicola/ngx-joyride/issues"
  },
  "homepage": "https://github.com/tnicola/ngx-joyride#readme",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@angular/common": ">=8.2.14",
    "@angular/core": ">=8.2.14"
  },
  "module": "fesm2015/ngx-joyride.mjs",
  "es2020": "fesm2020/ngx-joyride.mjs",
  "esm2020": "esm2020/ngx-joyride.mjs",
  "fesm2020": "fesm2020/ngx-joyride.mjs",
  "fesm2015": "fesm2015/ngx-joyride.mjs",
  "typings": "ngx-joyride.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./ngx-joyride.d.ts",
      "esm2020": "./esm2020/ngx-joyride.mjs",
      "es2020": "./fesm2020/ngx-joyride.mjs",
      "es2015": "./fesm2015/ngx-joyride.mjs",
      "node": "./fesm2015/ngx-joyride.mjs",
      "default": "./fesm2020/ngx-joyride.mjs"
    }
  },
  "sideEffects": false
}