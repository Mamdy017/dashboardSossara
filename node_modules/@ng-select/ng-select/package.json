{
  "name": "@ng-select/ng-select",
  "version": "10.0.4",
  "description": "Angular ng-select - All in One UI Select, Multiselect and Autocomplete",
  "author": "@ng-select/ng-select",
  "license": "MIT",
  "repository": "ng-select/ng-select",
  "engines": {
    "node": ">= 12.20.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "angular",
    "select",
    "ui-select",
    "dropdown",
    "multiselect",
    "autocomplete",
    "angular2"
