    "./scss/default.theme": {
      "style": "./scss/default.theme.scss"
    },
    "./scss/material.theme": {
      "style": "./scss/material.theme.scss"
    },
    "./themes/ant.design.theme.css": {
      "style": "./themes/ant.design.theme.css"
    },
    "./themes/default.theme.css": {
      "style": "./themes/default.theme.css"
    },
    "./themes/material.theme.css": {
      "style": "./themes/material.theme.css"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ng-select-ng-select.mjs",
      "es2020": "./fesm2020/ng-select-ng-select.mjs",
      "es2015": "./fesm2015/ng-select-ng-select.mjs",
      "node": "./fesm2015/ng-select-ng-select.mjs",
      "default": "./fesm2020/ng-select-ng-select.mjs"
    }
