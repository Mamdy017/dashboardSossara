    "tslib": "^2.3.1"
  },
  "module": "fesm2015/ng-select-ng-select.mjs",
  "es2020": "fesm2020/ng-select-ng-select.mjs",
  "esm2020": "esm2020/ng-select-ng-select.mjs",
  "fesm2020": "fesm2020/ng-select-ng-select.mjs",
